{
  "name": "@curiouslycory/memai-sdk",
  "version": "0.1.0-beta.1",
  "description": "TypeScript SDK for the mem.ai REST API",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run docs:api && tsc",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "docs:api": "typedoc"
  },
  "keywords": [
    "mem.ai",
    "sdk",
    "api",
    "client",
    "typescript"
  ],
  "author": "(Curiously) Cory Sougstad <cory@curiouslycory.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@9.15.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/CuriouslyCory/memai-sdk.git"
  },
  "homepage": "https://github.com/CuriouslyCory/memai-sdk",
  "bugs": {
    "url": "https://github.com/CuriouslyCory/memai-sdk/issues"
  },
  "devDependencies": {
    "@easyops-cn/docusaurus-search-local": "^0.49.2",
    "@eslint/js": "^9.26.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "docusaurus-plugin-typedoc": "^1.4.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.3",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
